{
    "model_name": "mi_cnn",
    "train_data_version": "v5b",
    "N_max": 60,
    "epochs": 1500,
    "patience": 150,
    "loss": "binary_crossentropy",
    "learning_rate": 1e-4,
    "beta_1": 0.99,
    "beta_2": 0.999,
    "weight_classes": "alert",
    "batch_size": 16,
    "metadata_cols": [
      "sgscore1",
      "distpsnr1",
      "sgscore2",
      "distpsnr2",
      "fwhm",
      "magpsf",
      "sigmapsf",
      "ra",
      "dec",
      "diffmaglim",
      "ndethist",
      "nmtchps",
      "age",
      "peakmag_so_far"
    ],
    "dropout_1": 0.20,
    "dropout_2": 0.35,
    "dropout_3": 0.45,
    "reduce_LR_factor": 0.4,
    "reduce_LR_minLR": 5e-10,
    "data_aug_h_flip": 1,
    "data_aug_v_flip": 1,
    "data_aug_rot": 1,

    "val_sne_only": 0,
    "val_keep_near_threshold": 1,
    "val_rise_only": 0,
    
    "random_seed": 2
  }
  
